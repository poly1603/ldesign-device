{
  "name": "@ldesign/device-vue",
  "version": "1.0.0",
  "description": "Vue 3 integration for @ldesign/device - Composables, components, directives and plugins for device detection",
  "keywords": [
    "vue3",
    "vue",
    "device",
    "detection",
    "responsive",
    "composables",
    "components",
    "directives",
    "battery",
    "network",
    "geolocation",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.cjs"
    },
    "./components": {
      "types": "./esm/components/index.d.ts",
      "import": "./esm/components/index.js",
      "require": "./cjs/components/index.cjs"
    },
    "./components/*": {
      "types": "./esm/components/*.d.ts",
      "import": "./esm/components/*.js",
      "require": "./cjs/components/*.cjs"
    },
    "./composables": {
      "types": "./esm/composables/index.d.ts",
      "import": "./esm/composables/index.js",
      "require": "./cjs/composables/index.cjs"
    },
    "./composables/*": {
      "types": "./esm/composables/*.d.ts",
      "import": "./esm/composables/*.js",
      "require": "./cjs/composables/*.cjs"
    },
    "./directives": {
      "types": "./esm/directives/index.d.ts",
      "import": "./esm/directives/index.js",
      "require": "./cjs/directives/index.cjs"
    },
    "./directives/*": {
      "types": "./esm/directives/*.d.ts",
      "import": "./esm/directives/*.js",
      "require": "./cjs/directives/*.cjs"
    },
    "./plugins": {
      "types": "./esm/plugins/index.d.ts",
      "import": "./esm/plugins/index.js",
      "require": "./cjs/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./esm/plugins/*.d.ts",
      "import": "./esm/plugins/*.js",
      "require": "./cjs/plugins/*.cjs"
    },
    "./cjs/components/DeviceInfo.vue.css": "./cjs/components/DeviceInfo.vue.css",
    "./cjs/components/NetworkStatus.vue.css": "./cjs/components/NetworkStatus.vue.css",
    "./cjs/index.css": "./cjs/index.css"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "esm",
    "cjs"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs",
    "build:watch": "ldesign-builder build -f esm,cjs --watch",
    "dev": "ldesign-builder build -f esm,cjs --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "clean": "rimraf es esm cjs dist coverage .tsbuildinfo"
  },
  "dependencies": {
    "@ldesign/device-core": "workspace:*",
    "@ldesign/shared": "workspace:*",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.5"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  }
}