{
  "name": "@ldesign/device",
  "version": "0.1.0",
  "description": "Modern device information detection library with Vue3 integration",
  "keywords": [
    "device",
    "detection",
    "responsive",
    "vue3",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/device#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldesign/device.git"
  },
  "bugs": {
    "url": "https://github.com/ldesign/device/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./engine": {
      "types": "./es/engine/index.d.ts",
      "import": "./es/engine/index.js",
      "require": "./lib/engine/index.cjs"
    },
    "./engine/*": {
      "types": "./es/engine/*.d.ts",
      "import": "./es/engine/*.js",
      "require": "./lib/engine/*.cjs"
    },
    "./modules/*": {
      "types": "./es/modules/*.d.ts",
      "import": "./es/modules/*.js",
      "require": "./lib/modules/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    },
    "./vue": {
      "types": "./es/vue/index.d.ts",
      "import": "./es/vue/index.js",
      "require": "./lib/vue/index.cjs"
    },
    "./vue/*": {
      "types": "./es/vue/*.d.ts",
      "import": "./es/vue/*.js",
      "require": "./lib/vue/*.cjs"
    },
    "./es/index.css": "./es/index.css",
    "./es/vue/components/DeviceInfo.vue.css": "./es/vue/components/DeviceInfo.vue.css",
    "./es/vue/components/NetworkStatus.vue.css": "./es/vue/components/NetworkStatus.vue.css",
    "./lib/index.css": "./lib/index.css",
    "./lib/vue/components/DeviceInfo.vue.css": "./lib/vue/components/DeviceInfo.vue.css",
    "./lib/vue/components/NetworkStatus.vue.css": "./lib/vue/components/NetworkStatus.vue.css",
    "./dist/index.css": "./dist/index.css",
    "./dist/index.min.css": "./dist/index.min.css"
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,umd",
    "build:watch": "ldesign-builder build --watch",
    "dev": "rollup -c -w",
    "type-check": "vue-tsc --noEmit --project tsconfig.test.json",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rimraf esm cjs dist types lib coverage .nyc_output",
    "size-check": "size-limit",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:check": "pnpm run build && node ../../tools/scripts/build/bundle-validator.js",
    "build:analyze": "pnpm run build && node ../../tools/scripts/build/bundle-analyzer.js",
    "build:validate": "pnpm run build && node ../../tools/scripts/build/validate-build.js",
    "build:browser-test": "pnpm run build && node ../../tools/scripts/build/browser-tester.js",
    "build:dev": "node scripts/build.js --dev"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@playwright/test": "^1.40.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.0.0",
    "jsdom": "^24.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.12",
    "vitepress": "^1.3.0",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5"
  },
  "engines": {
    "node": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "40 KB"
    }
  ],
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  }
}